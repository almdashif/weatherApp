{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    // other options
    "esModuleInterop": true,
    "baseUrl": "./src",
    "paths": {
      "@api/*": [
        "api/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@store/*": [
        "store/*"
      ]
    }
  }
}